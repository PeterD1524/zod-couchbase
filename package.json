{
  "name": "zod-couchbase",
  "version": "0.2.0",
  "description": "Object-document mapper for Couchbase using Zod",
  "homepage": "https://github.com/PeterD1524/zod-couchbase",
  "license": "MIT",
  "files": [
    "dist/src/**/*.d.ts",
    "dist/src/**/*.d.ts.map",
    "dist/src/**/*.js",
    "dist/src/**/*.js.map",
    "src"
  ],
  "exports": {
    ".": "./dist/src/index.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/PeterD1524/zod-couchbase"
  },
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "lint": "eslint ."
  },
  "dependencies": {
    "couchbase": "^4.6.0",
    "typescript-result": "^3.5.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "9.38.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@tsconfig/node22": "^22.0.2",
    "@tsconfig/strictest": "^2.0.7",
    "@types/node": "^22.18.13",
    "eslint": "9.38.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.6"
  }
}
